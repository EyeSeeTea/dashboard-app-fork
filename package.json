    "license": "BSD-3-Clause",
    "dependencies": {
        "@dhis2/analytics": "^23.10.0",
        "@dhis2/app-runtime": "^3.4.3",
        "@dhis2/app-runtime-adapter-d2": "^1.1.0",
        "@dhis2/d2-i18n": "^1.1.0",
        "@dhis2/d2-ui-core": "^7.3.3",
        "@dhis2/d2-ui-interpretations": "^7.3.3",
        "@dhis2/d2-ui-mentions-wrapper": "^7.3.3",
        "@dhis2/d2-ui-rich-text": "^7.3.3",
        "@dhis2/d2-ui-translation-dialog": "^7.3.3",
        "@dhis2/data-visualizer-plugin": "^39.0.7",
        "@dhis2/ui": "^8.2.6",
        "classnames": "^2.3.1",
        "cypress": "^9.7.0",
        "d2": "^31.10.0",
        "d2-utilizr": "^0.2.16",
        "i18next": "^20.3.5",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "react": "^16.14.0",
        "react-dom": "^16.13.1",
        "react-grid-layout": "1.2.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "redux": "^4.1.0",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "styled-jsx": "3.3.2"
    },
    "scripts": {
        "start": "d2-app-scripts start",
        "coverage": "npm test -- --coverage",
        "lint": "d2-style check",
        "build": "d2-app-scripts build",
        "build:netlify": "yarn build --standalone",
        "test": "d2-app-scripts test",
        "validate-push": "CI=true yarn test",
        "postinstall": "patch-package",
        "start-proxy": "d2-app-scripts start --proxy https://debug.dhis2.org/dev --proxyPort 8081",
        "start-cy-proxy": "BROWSER=none d2-app-scripts start --proxy https://debug.dhis2.org/dev --proxyPort 8081",
        "cypress:capture": "start-server-and-test 'yarn start-cy-proxy' http://localhost:3000 'yarn cypress run --env networkMode=capture'",
        "cypress:stub": "start-server-and-test 'yarn start-cy-proxy' http://localhost:3000 'yarn cypress run --env networkMode=stub'",
        "cypress:live": "start-server-and-test 'yarn start-cy-proxy' http://localhost:3000 'yarn cypress open --env networkMode=live'"
    },
    "devDependencies": {
        "@dhis2/cli-app-scripts": "^9.0.0",
        "@dhis2/cli-style": "^10.4.1",
        "@dhis2/cli-utils-cypress": "^9.0.2",
        "@dhis2/cypress-commands": "^9.0.2",
        "@dhis2/cypress-plugins": "^9.0.2",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "cypress-cucumber-preprocessor": "^4.3.1",
        "d2-manifest": "^1.0.0",
        "eslint-plugin-cypress": "^2.12.1",
        "immutability-helper": "^3.1.1",
        "patch-package": "^6.4.7",
        "postinstall-postinstall": "^2.1.0",
        "redux-mock-store": "^1.5.4",
        "start-server-and-test": "^1.14.0"
    },
    "resolutions": {
        "@dhis2/prop-types": "^1.6.4",
        "@dhis2/ui": "^8.2.6",
        "i18next": "^20.5.0"
    },
    "jest": {
        "moduleNameMapper": {
            "^.+\\.(css|sass|scss)$": "identity-obj-proxy"
        }
    }
}
